#include<bits/stdc++.h>
using namespace std;

void recur(vector < int > vc, int i)
{
    int sz = vc.size();
    if(i == sz)
    {
        cout << "i = " << i << " fucked up "<< endl;
        return;
    }
    for(int j = i; j < sz; ++j)
    {
            cout << "j = " << j << endl;
            recur(vc,j+1);
    }
}

int main()
{
    int t;
    while(cin >> t)
    {
        int n;
        cin >> n;
        vector < int > vc;
        for(int i = 0; i < n; ++i)
        {
            int x;
            cin >> x;
            vc.push_back(x);
        }
        recur(vc,0);
    }
    return 0;
}
